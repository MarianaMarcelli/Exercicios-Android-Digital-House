<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="0dp"
        android:left="-50dp"
        android:right="-50dp"
        android:top="-800dp">
        <rotate android:fromDegrees="45">
            <shape>
                <size
                    android:width="1dp"
                    android:height="10dp"/>
                <stroke
                    android:width="10dp"
                    android:color="#358165"/>
                <solid android:color="#69BE9E"/>
            </shape>
        </rotate>
    </item>
</layer-list>